<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".three">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar634"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="#02146A"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="제 3열람실"
        app:titleMargin="5dp"
        app:titleTextAppearance="@style/TextAppearance.AppCompat.Display1" />

    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="100dp"
        android:layout_marginBottom="250dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/btn1"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="1" />

            <Button
                android:id="@+id/btn2"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="2" />

            <Button
                android:id="@+id/btn3"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="3" />

            <Button
                android:id="@+id/btn4"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="4" />

            <Button
                android:id="@+id/btn5"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="5" />

            <Button
                android:id="@+id/btn6"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="6" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/btn7"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="7" />

            <Button
                android:id="@+id/btn8"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="8" />

            <Button
                android:id="@+id/btn9"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="9" />

            <Button
                android:id="@+id/btn10"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="10" />

            <Button
                android:id="@+id/btn11"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="11" />

            <Button
                android:id="@+id/btn12"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="12" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="30dp" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/btn13"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="13" />

            <Button
                android:id="@+id/btn14"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="14" />

            <Button
                android:id="@+id/btn15"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="15" />

            <Button
                android:id="@+id/btn16"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="16" />

            <Button
                android:id="@+id/btn17"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="17" />

            <Button
                android:id="@+id/btn18"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="18" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/btn19"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="19" />

            <Button
                android:id="@+id/btn20"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="20" />

            <Button
                android:id="@+id/btn21"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="21" />

            <Button
                android:id="@+id/btn22"
                android:layout_width="68dp"
                android:layout_height="70dp"
                android:text="22" />

            <Button
                android:id="@+id/btn23"
